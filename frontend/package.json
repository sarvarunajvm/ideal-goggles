{
  "name": "frontend",
  "version": "1.0.15",
  "description": "Frontend for Ideal Goggles application",
  "author": "Ideal Goggles Team",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest -c jest.config.js",
    "test:coverage": "jest -c jest.config.js --coverage",
    "test:p0": "jest -c jest.config.p0.js --coverage",
    "test:p0:quick": "jest -c jest.config.p0.js",
    "test:p1": "jest -c jest.config.p1.js --coverage",
    "test:p2": "jest -c jest.config.p2.js --coverage",
    "test:all": "npm run test:p0 && npm run test:p1 && npm run test:p2",
    "test:ci:quick": "npm run test:p0:quick",
    "test:ci:full": "npm run test:all",
    "test:unit": "jest -c jest.config.js tests/basic.test.js",
    "test:components": "jest -c jest.config.js --testPathPattern=tests/components/.*"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^16.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.11.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.5",
    "terser": "^5.44.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.3.6"
  }
}
